{
  "name": "CashBack Manager",
  "description": "Management of cashback",
  "version": "1.0.1",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["storage", "activeTab", "scripting", "notifications", "contextMenus"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "/images/cashback.png",
      "32": "/images/cashback.png",
      "48": "/images/cashback.png",
      "128": "/images/cashback.png"
    }
  },
  "icons": {
    "16": "/images/cashback.png",
    "32": "/images/cashback.png",
    "48": "/images/cashback.png",
    "128": "/images/cashback.png"
  },
  "options_page": "options.html"
}
